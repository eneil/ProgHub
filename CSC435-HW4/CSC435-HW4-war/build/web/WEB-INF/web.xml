<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>Control.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>Control.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListMembers</servlet-name>
        <servlet-class>Control.ListMembers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjectServlet</servlet-name>
        <servlet-class>Control.ProjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateProj</servlet-name>
        <servlet-class>Control.CreateProj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjTable</servlet-name>
        <servlet-class>Control.ProjTable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjCreate</servlet-name>
        <servlet-class>Control.ProjCreate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListMembers</servlet-name>
        <url-pattern>/ListMembers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProjectServlet</servlet-name>
        <url-pattern>/ProjectServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateProj</servlet-name>
        <url-pattern>/CreateProj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProjTable</servlet-name>
        <url-pattern>/ProjTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProjCreate</servlet-name>
        <url-pattern>/ProjCreate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
